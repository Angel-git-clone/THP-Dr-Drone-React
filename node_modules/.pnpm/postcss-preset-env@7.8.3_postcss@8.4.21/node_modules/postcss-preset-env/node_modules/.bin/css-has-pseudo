#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/dossier_linux/Drdrone/THP-Dr-Drone-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/dossier_linux/Drdrone/THP-Dr-Drone-react/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../css-has-pseudo@3.0.4_postcss@8.4.21/node_modules/css-has-pseudo/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../../../../../css-has-pseudo@3.0.4_postcss@8.4.21/node_modules/css-has-pseudo/dist/cli.cjs" "$@"
fi
